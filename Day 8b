int main()
{
    int a,b,c;
    printf("Enter three numbers ");
    scanf("%d%d%d",&a,&b,&c);
    if(a>b&&a>c)
    {
        printf("%d is Largest ",a);
    }
    else if(b>a&&b>c)
    {
         printf("%d is Largest ",b);
    }
    else if(c>a&&c>b)
    {
        printf("%d is Largest ",c);
    }
    else
    {
        printf("Default");
    }
    return 0;
}
