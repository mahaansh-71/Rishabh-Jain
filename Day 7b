#include<stdio.h>
int main()
{
    char ch;
    printf("Enter a alpha ");
    scanf("%c",&ch);
    if(ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U'||ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u')
    {
        printf("Character %c is a vowel ",ch);
    }
    else
    {
         printf("Character %c is a consonent",ch);
    }
    return 0;
}
