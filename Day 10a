#include<stdio.h>
int main()
{
    int S1,S2,S3;
    printf("Input Sides ");
    scanf("%d%d%d",&S1,&S2,&S3);
    if(S1==S2&&S2==S3)
    {
        printf("Equilateral Triangle ");
    }
    else if((S1==S2&&S2!=S3)||(S1==S3&&S3!=S2))
    {
         printf("Isosceles Triangle ");
    }
    else
    {
        printf("Scalene Triangle");
    }
    return 0;
}
