#include<stdio.h>
int main()
{
    float P,PP,L,LP,SP,CP;
    printf("Enter the Prize ");
    scanf("%f%f",&CP,&SP);
    if(SP>CP)
    {
        P=SP-CP;
        PP=(P/CP)*100;
        printf("Profit Percentage % = %f",PP);
    }
    else if(CP>SP)
    {
        L=CP-SP;
        LP=(L/CP)*100;
        printf("Loss Percentage % = %f",LP);
    }
    else
    {
        printf("Theft");
    }
    return 0;
}
           
